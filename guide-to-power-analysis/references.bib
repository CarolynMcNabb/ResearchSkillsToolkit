  pmid = {34334720},
  keywords = {Adult,Athletic Performance,Double-Blind Method,Fructose,Gastrointestinal Tract,Glucose,Humans,Hydrogels,Male,Oxidation-Reduction,Performance-Enhancing Substances,Running,Young Adult},
