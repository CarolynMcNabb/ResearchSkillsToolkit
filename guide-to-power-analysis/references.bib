@article{grgicCaffeineIngestionEnhances2018,
  ids = {grgicCaffeineIngestionEnhances2018a,grgicCaffeineIngestionEnhances2018b},
  title = {Caffeine Ingestion Enhances {{Wingate}} Performance: A Meta-Analysis.},
  author = {Grgic, Jozo},
