  keywords = {Clinical psychology,Decision trees,Psychology,Publication ethics,Scientific publishing,Social psychology,Statistical data,Test statistics},
  file = {/Users/cristian/Zotero/storage/6P4SNBIQ/Kühberger et al. - 2014 - Publication Bias in Psychology A Diagnosis Based .pdf;/Users/cristian/Zotero/storage/XQ4I545U/article.html}
