@article{bartosZcurve20Estimating2022,
  title = {Z-Curve 2.0: {{Estimating Replication Rates}} and {{Discovery Rates}}},
  shorttitle = {Z-Curve 2.0},
  author = {Barto{\v s}, Franti{\v s}ek and Schimmack, Ulrich},
  year = {2022},
